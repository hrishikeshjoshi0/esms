	<parameter name="quote" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     quote.`id` AS quote_id,
     quote.`version` AS quote_version,
     quote.`assigned_to` AS quote_assigned_to,
     quote.`contact_name` AS quote_contact_name,
